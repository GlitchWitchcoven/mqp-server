{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "max-len": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "consistent-return": 0,
    "func-names": 0,
    "indent": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  }
}